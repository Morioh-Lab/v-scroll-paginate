{
  "name": "v-scroll-paginate",
  "version": "1.0.0",
  "description": "An infinite scroll paginate plugin for Vue 3.0",
  "module": "./src/index.js",
  "main": "./dist/scroll-paginate.min.js",
  "unpkg": "./dist/scroll-paginate.min.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development --progress --watch",
    "prod": "del ./dist && cross-env NODE_ENV=production webpack --mode production --watch --progress",
    "start": "cd ./dist && node server.js",
    "pub": "npm publish --access public"
  },
  "browserslist": [
    "last 2 version",
    "> 2%"
  ],
  "keywords": [
    "vue infinite loading",
    "infinite loading",
    "vue scroll paginate",
    "scroll paginate",
    "vue paginate component",
    "paginate scroll",
    "paginate component"  
  ],
  "author": "Morioh Team <support@morioh.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Morioh-Lab/v-scroll-paginate.git"
  },
  "bugs": {
    "url": "https://github.com/Morioh-Lab/v-scroll-paginate/issues"
  },
  "homepage": "https://github.com/Morioh-Lab/v-scroll-paginate",
